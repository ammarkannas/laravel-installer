{
    "name": "rwx-rwx/installer",
    "description": "Laravel web installer",
    "license": "MIT",
    "authors": [
        {
            "name": "Ammar A Kannas",
            "email": "ammarkannas@outlook.com"
        }
    ],
    "require": {
        "php": ">=8.0"
    },
    "autoload": {
        "psr-4": {
            "Rwxrwx\\Installer\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rwxrwx\\Installer\\Providers\\InstallerServiceProvider"
            ],
            "aliases": {
                "Installer": "Rwxrwx\\Installer\\Facades\\Installer"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable"
}
